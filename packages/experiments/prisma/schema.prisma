datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

generator client {
    provider = "prisma-client-js"
}

model Guild {
    id                       String       @id
    forceEnabledExperiments  Experiment[] @relation("force_enabled")
    forceDisabledExperiments Experiment[] @relation("force_disabled")
}

model Experiment {
    feature_key        String  @id @unique
    name               String
    rollout_percentage Int
    force_enabled      Guild[] @relation("force_enabled")
    force_disabled     Guild[] @relation("force_disabled")
}
